#!/bin/bash
PYTHON_LIB=/usr/lib/python3.2
#IMPORTLIB=$PYTHON_LIB/importlib/_bootstrap.py
IMPORTLIB=fake-importlib.py
SIXTYFOUR_BIT="ListItem(64) ListItem(2305843009213693951) ListItem(314159) ListItem(0) ListItem(1000003)"
DEBUG=true
HASHINFO=$SIXTYFOUR_BIT
OS=POSIX
GC_THRESHOLD=100
krun --parser=./parser --output-mode raw -cHASHINFO="$HASHINFO" -cDEBUG="$DEBUG" -cOS="$OS" -cGCTHRESHOLD="$GC_THRESHOLD" --cfg-parser="./parser" -cIMPORTLIB="$IMPORTLIB" $@
